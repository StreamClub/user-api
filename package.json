{
  "name": "process-api",
  "version": "0.0.1",
  "description": "Stream Club Process API",
  "main": "app.ts",
  "scripts": {
    "start": "npx nodemon -e ts --exec \"npx ts-node -r tsconfig-paths/register src/index.ts\"",
    "build": "tsc && npm run generate-tscpaths",
    "generate-tscpaths": "mkdir -p ./dist/server && tscpaths -p tsconfig.json -s ./src -o ./dist/server",
    "test": "jest --coverage"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.5.1",
    "clone-deep": "^4.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "express": "^4.18.2",
    "http-status-code": "^2.1.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.10.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "moment": "^2.29.4",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.7",
    "nodemon": "^2.0.15",
    "password-hash": "^1.2.2",
    "pg": "^8.11.3",
    "prettier": "^3.0.3",
    "sequelize": "^6.33.0",
    "sqlite3": "^5.1.6",
    "supertest": "^6.3.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-typescript": "^7.23.3",
    "@types/clone-deep": "^4.0.2",
    "@types/cors": "^2.8.15",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.8.8",
    "@types/node-schedule": "^2.1.3",
    "@types/nodemailer": "^6.4.14",
    "@types/password-hash": "^1.2.22",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "3.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.3.2"
  },
  "_moduleAliases": {
    "@root/*": "src/*",
    "@root": "src",
    "@services": "src/services",
    "@services*": "src/services/*",
    "@controllers": "src/controllers",
    "@controllers/*": "src/controllers/*",
    "@config": "src/config",
    "@config/*": "src/config/*",
    "@middlewares": "src/middlewares",
    "@middlewares/*": "src/middlewares/*",
    "@routes": "src/routes",
    "@routes/*": "src/routes/*",
    "@models": "src/models",
    "@models/*": "src/models/*",
    "@dtos": "src/dtos",
    "@dtos/*": "src/dtos/*",
    "@exceptions": "src/exceptions",
    "@exceptions/*": "src/exceptions/*",
    "@entities": "src/entities",
    "@entities/*": "src/entities/*",
    "@dal": "./src/dal",
    "@dal/*": "./src/dal/*",
    "@utils": "src/utils",
    "@utils/*": "src/utils/*",
    "@handlers": "src/handlers",
    "@handlers/*": "src/handlers/*"
  },
  "jest": {
    "moduleNameMapper": {
      "@dal": "<rootDir>/src/dal",
      "@dal/(.*)": "<rootDir>/src/dal/$1",
      "@root/*": "<rootDir>/src/*",
      "@root": "<rootDir>/src",
      "@services": "<rootDir>/src/services",
      "@services*": "<rootDir>/src/services/*",
      "@controllers": "<rootDir>/src/controllers",
      "@controllers/*": "<rootDir>/src/controllers/*",
      "@config": "<rootDir>/src/config",
      "@config/*": "<rootDir>/src/config/*",
      "@middlewares": "<rootDir>/src/middlewares",
      "@middlewares/*": "<rootDir>/src/middlewares/*",
      "@routes": "<rootDir>/src/routes",
      "@routes/*": "<rootDir>/src/routes/*",
      "@models": "<rootDir>/src/models",
      "@models/*": "<rootDir>/src/models/*",
      "@dtos": "<rootDir>/src/dtos",
      "@dtos/*": "<rootDir>/src/dtos/*",
      "@exceptions": "<rootDir>/src/exceptions",
      "@exceptions/*": "<rootDir>/src/exceptions/*",
      "@entities": "<rootDir>/src/entities",
      "@entities/*": "<rootDir>/src/entities/*",
      "@utils": "<rootDir>/src/utils",
      "@utils/*": "<rootDir>/src/utils/*",
      "@handlers": "<rootDir>/src/handlers",
      "@handlers/*": "<rootDir>/src/handlers/*"
    }
  }
}
